[OpenHPC_{{ release }}_Factory]
name=Rolling development build for {{ release }}
type=rpm-md
{% if distro.startswith('rocky') %}
baseurl=http://obs.openhpc.community:82/OpenHPC3:/{{ release }}:/Factory/EL_9/
{% endif %}
{% if distro.startswith('almalinux') %}
baseurl=http://obs.openhpc.community:82/OpenHPC3:/{{ release }}:/Factory/EL_9/
{% endif %}
{% if distro.startswith('openEuler') %}
baseurl=http://obs.openhpc.community:82/OpenHPC3:/{{ release }}:/Factory/{{ distro }}/
{% endif %}
{% if distro.startswith('leap') %}
baseurl=http://obs.openhpc.community:82/OpenHPC3:/{{ release }}:/Factory/Leap_15/
{% endif %}
gpgcheck=0
enabled=1
